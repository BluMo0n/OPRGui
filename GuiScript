-- Scripts:

local function AAAGOE_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.Parent.Parent:Destroy()
	end)

end
coroutine.wrap(AAAGOE_fake_script)()
local function HHTNZ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(HHTNZ_fake_script)()
local function ZTHFET_fake_script() -- ChestFarm.ChestClient 
	local script = Instance.new('LocalScript', ChestFarm)

	local ChestLists = game.Workspace[".Chests"]
	local Chests = ChestLists:GetChildren()
	local Player = game.Players.LocalPlayer

	script.Parent.Activated:Connect(function()
		for i,v in pairs(Chests) do
			wait(.1)
			if v.Name == "SmallChest" or v.Name == "LargeChest" then
				print(v.Name)
				local ChestPos = v.ChestBase.CFrame
				local PlayerPos = Player.Character.HumanoidRootPart.CFrame
				Player.Character.HumanoidRootPart.CFrame = ChestPos
				wait(.1)
				Player.Character.HumanoidRootPart.CFrame = PlayerPos
			end
		end
	end)
end
coroutine.wrap(ZTHFET_fake_script)()
local function TQTKRM_fake_script() -- FruitSniper.FruitClient 
	local script = Instance.new('LocalScript', FruitSniper)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer

	local Items = {
		"Bat Bat",
		"Bomb Bomb",
		"Night Night",
		"Chop Chop",
		"Revive Revive",
		"Rubber Rubber",
		"Spring Spring",
		"Stone Stone",
		"Gold Gold",
		"Dice Dice",
		"Paw Paw",
		"Snow",
		"Blood Blood",
		"Swamp Swamp",
		"Mochi Mochi",
		"String String",
		"Phoenix Phoenix",
		"Operation Operation",
		"Nano Nano",
		"Hollow Hollow",
		"Barrier Barrier",
		"Flame Flame",
		"Shine Shine",
		"Magma Magma",
		"Dark Dark",
		"Sand Sand",
		"Thunder Thunder",
		"Ice Ice",
		"Quake Quake",
		"Buddha Buddha",
		"Venom Venom",
	}
	script.Parent.Activated:Connect(function()
		for i,v in pairs(Works) do
			if table.find(Items, v.Name) then
				local PlayerPos = Player.Character.HumanoidRootPart.CFrame
				wait(.1)
				print(v.Name)
				local HandlePos = v.Handle.CFrame
				Player.Character.HumanoidRootPart.CFrame = HandlePos
				wait(.1)
				Player.Character.HumanoidRootPart.CFrame = PlayerPos
			else
				print("Not Found")
			end
		end
	end)

end
coroutine.wrap(TQTKRM_fake_script)()
local function JLIAX_fake_script() -- WaterWalking.WaterClient 
	local script = Instance.new('LocalScript', WaterWalking)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer
	local d = true

	script.Parent.Activated:Connect(function()
		if d == true then
			for i,v in pairs(Works) do
				if v.ClassName == "Model" and v:FindFirstChild("SEA1") then
					v.SEA1.CanCollide = true
				end
			end
			d = false
		elseif d == false then
			for i,v in pairs(Works) do
				if v.ClassName == "Model" and v:FindFirstChild("SEA1") then
					v.SEA1.CanCollide = false
				end
			end
			d = true
		end
	end)
end
coroutine.wrap(JLIAX_fake_script)()
