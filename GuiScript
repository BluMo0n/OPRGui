-- Scripts:

local function VJOYB_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.Parent.Parent:Destroy()
	end)
	
end
coroutine.wrap(VJOYB_fake_script)()
local function UDJS_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(UDJS_fake_script)()
local function RTQCL_fake_script() -- ChestFarm.ChestClient 
	local script = Instance.new('LocalScript', ChestFarm)

	local ChestLists = game.Workspace[".Chests"]
	local Chests = ChestLists:GetChildren()
	local Player = game.Players.LocalPlayer
	
	script.Parent.Activated:Connect(function()
		for i,v in pairs(Chests) do
			wait(.1)
			if v.Name == "SmallChest" or v.Name == "LargeChest" then
				print(v.Name)
				local ChestPos = v.ChestBase.Position
				local PlayerPos = Player.Character.HumanoidRootPart.Position
				Player.Character.HumanoidRootPart.Position = ChestPos
				wait(.1)
				Player.Character.HumanoidRootPart.Position = PlayerPos
			end
		end
	end)
end
coroutine.wrap(RTQCL_fake_script)()
local function RYSKR_fake_script() -- FruitSniper.FruitClient 
	local script = Instance.new('LocalScript', FruitSniper)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer
	
	local Items = {
		"Bat Bat",
		"Bomb Bomb",
		"Night Night",
		"Chop Chop",
		"Revive Revive",
		"Rubber Rubber",
		"Spring Spring",
		"Stone Stone",
		"Gold Gold",
		"Dice Dice",
		"Paw Paw",
		"Snow",
		"Blood Blood",
		"Swamp Swamp",
		"Mochi Mochi",
		"String String",
		"Phoenix Phoenix",
		"Operation Operation",
		"Nano Nano",
		"Hollow Hollow",
		"Barrier Barrier",
		"Flame Flame",
		"Shine Shine",
		"Magma Magma",
		"Dark Dark",
		"Sand Sand",
		"Thunder Thunder",
		"Ice Ice",
		"Quake Quake",
		"Buddha Buddha",
		"Venom Venom",
	}
	script.Parent.Activated:Connect(function()
		for i,v in pairs(Works) do
			if table.find(Items, v.Name) then
				local PlayerPos = Player.Character.HumanoidRootPart.Position
				wait(.1)
				print(v.Name)
				local HandlePos = v.Handle.Position
				Player.Character.HumanoidRootPart.Position = HandlePos
				wait(.1)
				Player.Character.HumanoidRootPart.Position = PlayerPos
			else
				print("Not Found")
			end
		end
	end)
	
end
coroutine.wrap(RYSKR_fake_script)()
local function GGXFNWY_fake_script() -- WaterWalking.FruitClient 
	local script = Instance.new('LocalScript', WaterWalking)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer
	
	script.Parent.Activated:Connect(function()
		for i,v in pairs(Works) do
			if v.ClassName == "Model" and v:FindFirstChild("SEA1") then
				v.SEA1.CanCollide = true
			end
		end
	end)
	
end
coroutine.wrap(GGXFNWY_fake_script)()
local function LAIK_fake_script() -- NormalSpeed.FruitClient 
	local script = Instance.new('LocalScript', NormalSpeed)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer
	
	script.Parent.Activated:Connect(function()
		Player.Character.Humanoid.WalkSpeed = 16
	end)
	
end
coroutine.wrap(LAIK_fake_script)()
local function GFDV_fake_script() -- FastSpeed.FruitClient 
	local script = Instance.new('LocalScript', FastSpeed)

	local Works = game.Workspace:GetChildren()
	local Player = game.Players.LocalPlayer
	
	script.Parent.Activated:Connect(function()
		Player.Character.Humanoid.WalkSpeed = 100
	end)
	
end
coroutine.wrap(GFDV_fake_script)()
